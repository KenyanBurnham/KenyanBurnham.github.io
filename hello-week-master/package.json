{
  "name": "hello-week",
  "version": "1.4.0",
  "description": "Lightweight and simple calendar with no dependencies in pure JavaScript",
  "license": "MIT",
  "repository": "https://github.com/maurovieirareis/hello-week",
  "keywords": [
    "yarn",
    "webpack",
    "typescript",
    "javascript",
    "scss",
    "datepicker",
    "calendar"
  ],
  "author": "Mauro Reis Vieira <mauroreisvieira@gmail.com>",
  "bugs": {
    "url": "https://github.com/maurovieirareis/hello-week/issues"
  },
  "ignore": [
    "node_modules"
  ],
  "scripts": {
    "watch": " webpack --watch --progress --mode development",
    "start": "node_modules/.bin/webpack-dev-server --config webpack.config.js --open --mode development",
    "build": "node_modules/.bin/webpack --config webpack.config.js --mode production",
    "lint:ts": "./node_modules/.bin/tslint -c tslint.json -p ./",
    "lint:scss": "stylelint src/**/*.scss",
    "test": "./node_modules/.bin/mocha --compilers ts:ts-node/register -r ignore-styles -r jsdom-global/register __tests__/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "source-map-loader": "^0.2.3",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.1.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.1",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "webpack-cli": "^2.0.14",
    "css-loader": "^0.28.11",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^7.0.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.20.3",
    "stylelint-config-standard": "^18.2.0",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.3"
  }
}
